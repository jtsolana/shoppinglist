{
  "name": "shoppinglist",
  "version": "1.0.0",
  "description": "Shopping list deskto app",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "author": "\"JT Solana\"",
  "license": "ISC",
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "knex": "^2.0.0",
